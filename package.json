{
  "name": "first",
  "displayName": "first",
  "description": "First Extension Tutorial",
  "version": "1.0",
  "publisher": "Tejas",
  "repository": "https://github.com/tejasnamjoshi/vscode-sync.git",
  "engines": {
    "vscode": "^1.29.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.readExtensionsList",
    "onCommand:extension.saveExtensionsList"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.readExtensionsList",
        "title": "Read Extensions"
      },
      {
        "command": "extension.saveExtensionsList",
        "title": "Save Extensions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.21",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
